{
  "token": {
    "expires_in": 3600
  },
  "session": {
    "saveUninitialized": true,
    "resave": true
  },
  /**
   * Cross Domain Setting
   */
  "cross": {
    "enable": true,
    "allow_origin" : null, // default: "*",
    "allow_headers" : null // default: Origin, X-Requested-With, Content-Type, Accept
  },
  "cache": {
        "enable": true,
        "adapter": "memory",
        "backup": "redis",
        "redis": {
            "port": "6379",
            "db": 0,
            "ttl": 86400
        },
        "memory": {
            "max": 100,
            "ttl": 60
        }
    }
}